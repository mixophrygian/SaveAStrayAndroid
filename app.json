{
  "expo": {
    "name": "Save a Stray",
    "description": "An app to help you locate animal rescue shelters near you",
    "slug": "save-a-stray",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "2.1.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/bluepuppy.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "icon": "./assets/AppIcon/appicon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.strayrescue",
      "supportsTablet":false 
    },
    "android": {
      "package": "com.strayrescue"
    }
  }
}
